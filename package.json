{
  "name": "ccbot",
  "displayName": "Custom Command bot",
  "description": "Make Visual Studio help you with developing custom command bot commands!",
  "version": "3.6.0",
  "publisher": "fajfaj",
  "icon": "./language/assets/extension.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/fajfaj1/vsc-ccbot-extension"
  },
  "keywords": [
    "custom command bot",
    "syntaxes",
    "Language",
    "Grammars",
    "custom command",
    "discord",
    "ccbot",
    "custom commands"
  ],
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages",
    "Formatters",
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ccbot",
        "aliases": [
          "Custom Command",
          "ccbot"
        ],
        "extensions": [
          ".ccbot",
          ".cb",
          ".customcommand",
          ".customcommandbot"
        ],
        "configuration": "./language/configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ccbot",
        "scopeName": "source.ccbot",
        "path": "./language/grammars.json"
      }
    ]
  },
  "capabilities": {
    "completionProvider": {
      "resolveProvider": "true",
      "triggerCharacters": [
        "$"
      ],
      "signatureHelpProvider" : {
        "triggerCharacters": [ "[" ]
    }
    }
  },
    "dependencies": {
      "git": "^0.1.5"
    },
    "scripts": {
      "data": "node ./language/modules/parser.js",
      "change": "node ./scripts/changelog.js"
    },
    "main": "./language/main.js"
  }